{
  "name": "ingestor-events",
  "description": "Ingestor Events - Pulls data from specific sources",
  "version": "1.0.0",
  "main": "main.js",
  "license": "MIT",
  "scripts": {
    "babel": "babel src -d build",
    "build": "yarn lint && yarn babel && yarn docker",
    "dev": "nodemon build/server.js",
    "docker": "docker build -t jgarciadiaz/ingestor-events .",
    "grpn": "node build/groupon",
    "lint": "esw src",
    "postinstall": "yarn build",
    "run-docker": "docker run -e 'DB_URL=mongodb://mongo/events' --name ingestor-events --link mongodb:mongo -d -p 49161:8080 jgarciadiaz/ingestor-events",
    "start": "yarn grpn"
  },
  "dependencies": {
    "convict": "^4.0.1",
    "mongoose": "^4.13.1",
    "request": "^2.83.0",
    "request-promise-native": "^1.0.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "7",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-runtime": "^6.26.0",
    "eslint": "^4.11.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-watch": "^3.1.3"
  }
}
