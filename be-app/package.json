{
  "name": "be-app",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "babel": "babel src -d build",
    "build": "yarn lint && yarn babel && yarn docker",
    "dev": "nodemon build/server",
    "docker": "docker build -t jgarciadiaz/api-events .",
    "start": "node ./build/server",
    "lint": "esw src",
    "postinstall": "yarn build",
    "run-docker": "docker run -e 'DB_URL=mongodb://mongo/events' --name api-events --link mongodb:mongo -d -p 49160:8000 jgarciadiaz/api-events"
  },
  "dependencies": {
    "convict": "^4.0.1",
    "express": "^4.16.2",
    "express-graphql": "^0.6.11",
    "graphql": "^0.11.7",
    "material-ui": "^0.19.4",
    "mongoose": "^4.13.2",
    "morgan": "^1.9.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.11.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-watch": "^3.1.3",
    "nodemon": "^1.12.1"
  }
}
